{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": "off",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": [
      "error",
      { "endOfLine": "auto" }
    ]
  }
}
